// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+shift+alt+i",
        "command": "editor.action.smartSelect.expand",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+alt+8",
        "command": "editor.action.smartSelect.shrink",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+left",
        "command": "-editor.action.smartSelect.shrink",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+i",
        "command": "editor.action.selectToBracket"
    },
    {
        "key": "ctrl+u",
        "command": "editor.action.formatSelection",
        "when": "editorHasDocumentSelectionFormattingProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k ctrl+f",
        "command": "-editor.action.formatSelection",
        "when": "editorHasDocumentSelectionFormattingProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+n",
        "command": "extension.selectParenthesis",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k shift+9",
        "command": "-extension.selectParenthesis",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+alt+z",
        "command": "extension.selectSquareBrackets",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k oem_4",
        "command": "-extension.selectSquareBrackets",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k 7",
        "command": "extension.selectCurlyBrackets",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k shift+oem_4",
        "command": "-extension.selectCurlyBrackets",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+alt+oem_102",
        "command": "extension.selectAngleBrackets",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k shift+oem_comma",
        "command": "-extension.selectAngleBrackets",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+alt+h",
        "command": "extension.selectCurlyBracketsOuter",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k shift+oem_6",
        "command": "-extension.selectCurlyBracketsOuter",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k shift+9",
        "command": "extension.selectParenthesisOuter",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k shift+0",
        "command": "-extension.selectParenthesisOuter",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k 9",
        "command": "extension.selectSquareBracketsOuter",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k oem_6",
        "command": "-extension.selectSquareBracketsOuter",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k shift+2",
        "command": "extension.selectDoubleQuote",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k shift+oem_7",
        "command": "-extension.selectDoubleQuote",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.view.explorer",
        "when": "viewContainer.workbench.view.explorer.enabled"
    },
    {
        "key": "ctrl+shift+alt+6",
        "command": "extension.selectEitherQuote",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+k oem_1",
        "command": "-extension.selectEitherQuote",
        "when": "editorFocus"
    },
    {
        "key": "ctrl+shift+alt+m",
        "command": "editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+down",
        "command": "-editor.action.copyLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+j",
        "command": "editor.action.copyLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+alt+up",
        "command": "-editor.action.copyLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+q",
        "command": "-workbench.action.quickOpenView"
    },
    {
        "key": "ctrl+q",
        "command": "-workbench.action.quickOpenNavigateNextInViewPicker",
        "when": "inQuickOpen && inViewsPicker"
    },
    {
        "key": "ctrl+q",
        "command": "workbench.action.closeActiveEditor"
    },
    {
        "key": "ctrl+w",
        "command": "-workbench.action.closeActiveEditor"
    },
    {
        "key": "ctrl+shift+alt+7",
        "command": "editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+up",
        "command": "-editor.action.insertCursorAbove",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+alt+u",
        "command": "editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+down",
        "command": "-editor.action.insertCursorBelow",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+up",
        "command": "-cursorUpSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+up",
        "command": "-workbench.action.terminal.selectToPreviousCommand",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+shift+up",
        "command": "-notebook.cell.focusOutOutput",
        "when": "notebookEditorFocused && notebookOutputFocused"
    },
    {
        "key": "ctrl+shift+down",
        "command": "-notebook.cell.focusInOutput",
        "when": "notebookCellHasOutputs && notebookEditorFocused"
    },
    {
        "key": "ctrl+shift+down",
        "command": "-workbench.action.terminal.selectToNextCommand",
        "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
    },
    {
        "key": "ctrl+shift+down",
        "command": "-cursorDownSelect",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+shift+alt+k",
        "command": "editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+down",
        "command": "-editor.action.moveLinesDownAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+l",
        "command": "editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+up",
        "command": "-editor.action.moveLinesUpAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+alt+oem_2",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+oem_2",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    }
]